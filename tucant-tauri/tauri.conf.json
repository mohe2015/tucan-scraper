{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tucant-tauri",
  "version": "0.1.0",
  "identifier": "de.selfmade4u.tucant",
  "build": {
    "beforeDevCommand": {
      "cwd": "../tucant-yew",
      "script": "trunk serve"
    },
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": {
      "cwd": "../tucant-yew",
      "script": "trunk build"
    },
    "frontendDist": "../tucant-yew/dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "tucant-tauri",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}